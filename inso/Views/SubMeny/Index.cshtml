   <script type="text/javascript">
       function crecer(x) {
           x.style.width = "52.5%";
       }

       function normal(x) {
           x.style.width = "51%";
       }

       function crec(x) {
           x.style.width = "46%";
       }

       function nor(x) {
           x.style.width = "44%";
       }
   </script>
</head>

<body onload=" document.getElementById('navbar').style.display = 'none';">
